{
  "name": "masm-x86-runner",
  "version": "1.0.0",
  "description": "VSCode extension for compiling and running x86 Microsoft Assembly Language (MASM)",
  "main": "./build/electron-main.js",
  "scripts": {
    "build": "gulp",
    "dev": "gulp watch",
    "build-electron": "gulp electron",
    "build-installer": "electron-builder",
    "deploy": "electron-builder build -mwl --publish always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/istareatscreens/vscode-masm-x86-runner"
  },
  "keywords": [
    "MASM",
    "vscode",
    "compiler",
    "linker"
  ],
  "author": "istareatscreens",
  "homepage": "https://github.com/istareatscreens/GeneticMazeSolver#readme",
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^9.0.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.13",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "del": "^6.0.0",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-better-rollup": "^4.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-livereload": "^4.0.2",
    "gulp-postcss": "^9.0.0",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.0.1",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-merge": "^5.7.3",
    "webpack-stream": "^6.1.1"
  },
  "dependencies": {
    "codemirror": "^5.59.1",
    "electron-updater": "^4.3.5",
    "eol": "^0.9.1",
    "file-saver": "^2.0.5",
    "gulp-htmlmin": "^5.0.1",
    "react": "^17.0.1",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.1",
    "react-draggable": "^4.4.3",
    "react-onclickoutside": "^6.10.0",
    "use-debounce": "^5.2.1"
  },
  "build": {
    "appId": "com.isas.masm-x86-runner",
    "productName": "MASM x86 Runner",
    "extends": null,
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icon.ico"
    },
    "files": [
      "./build/*",
      "package.json"
    ],
    "nsis": {
      "uninstallDisplayName": "WASM MASM x86 Editor",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "artifactName": "${productName}-${version}-osx.${ext}",
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "icon.icns"
    },
    "linux": {
      "artifactName": "${productName}-${version}-${arch}-linux.${ext}",
      "maintainer": "istareatscreens",
      "target": [
        "tar.gz",
        "deb",
        "rpm",
        "pacman"
      ]
    }
  }
}
